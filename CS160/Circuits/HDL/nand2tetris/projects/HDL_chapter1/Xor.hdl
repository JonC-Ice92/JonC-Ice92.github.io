/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=b, out=n1);
    Not(in=a, out=n2);
    Add(a=a, b=n1, out=w1);
    Add(a=n2, b=b, out=w2);
    Or(a=w1, b=w2, out=out);
}